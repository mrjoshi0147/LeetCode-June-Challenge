class Solution {
   public boolean isSubsequence(String s, String t) {
        int s1=0;
        
      int i=0; 
            for(int j=0;j<t.length();j++){
                if(i<s.length()){
                if(s.charAt(i)==t.charAt(j)){
                    s1++;
                    i++;
                }
                }
                
            }
    
        
        if(s1 == s.length()){
            return true;
        }
        
        return false;
    }
}
